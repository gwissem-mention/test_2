{% if object.description is defined and object.category.vars.value  is same as("1") %}
    <div class="fr-mb-4v">
        {% embed 'embed/counter.html.twig' with {
            'parent': 'objects_objects_0_description',
            'minlength': object.description.vars.attr.minlength,
            'maxlength': object.description.vars.attr.maxlength,
        } %}{% block label %}
            {{ form_label(object.description) }}
        {% endblock %}
            {% block widget %}
                {{ form_widget(object.description) }}
            {% endblock %}
            {% block errors %}
                {{ form_errors(object.description) }}
            {% endblock %}
        {% endembed %}
    </div>
{% endif %}
{% if object.documentType.documentAdditionalInformation.documentOwnerAddress is defined %}
    {% set etalab_errors = [] %}
    {% if object.documentType.documentAdditionalInformation.documentOwnerAddress is defined %}
        {% for error in object.documentType.documentAdditionalInformation.documentOwnerAddress.address.vars.errors %}
            {% set etalab_errors = etalab_errors|merge([error.message]) %}
        {% endfor %}
    {% endif %}
    <div class="fr-input-group{% if etalab_errors|length > 0 %} fr-input-group--error{% endif %}">
        {{ form_label(object.documentType.documentAdditionalInformation.documentOwnerAddress, null, {'label_attr': {'for':'document_owner_address_' ~ loop.index0}}) }}
        {{ component('etalab_component', {
            'data-live-id': 'document-owner-address-' ~loop.index0,
            dataModel: 'addressSearch:documentOwnerAddresses[' ~ loop.index0 ~ '][addressSearch] addressId:documentOwnerAddresses[' ~ loop.index0 ~ '][addressId] addressSearchSaved:documentOwnerAddresses[' ~ loop.index0 ~ '][addressSearchSaved]',
            inputName: 'document_owner_address_' ~ loop.index0,
            dataGirondeEnabled : false,
            errors: etalab_errors,
        }) }}
    </div>
{% endif %}
{% if object.documentType.documentNumber is defined %}
    {{ form_row(object.documentType.documentNumber) }}
{% endif %}
