<footer role="contentinfo" class="fr-footer" id="footer">
  <div class="fr-container">
    <div class="fr-footer__body">
      <div class="fr-footer__brand fr-enlarge-link">
          <a href="/" title="Retour à l’accueil">
              <p class="fr-logo">
                  {{ 'pel.footer.logo'|trans|raw }}
              </p>
          </a>
      </div>
      <div class="fr-footer__content fr-pl-md-3v">
        <p class="fr-footer__content-desc">
            {{ 'pel.footer.description'|trans }}
        </p>
        <ul class="fr-footer__content-list">
          <li class="fr-footer__content-item">
            <a class="fr-footer__content-link" title="{{ 'pel.footer.link.legifrance.title'|trans }}" rel="noreferrer noopener" target="_blank" href="https://www.legifrance.gouv.fr/">{{ 'pel.footer.link.legifrance'|trans }}</a>
          </li>
          <li class="fr-footer__content-item">
            <a class="fr-footer__content-link" title="{{ 'pel.footer.link.gouvernement.title'|trans }}" rel="noreferrer noopener" target="_blank" target="_blank" href="https://www.gouvernement.fr/">{{ 'pel.footer.link.gouvernement'|trans }}</a>
          </li>
          <li class="fr-footer__content-item">
            <a class="fr-footer__content-link" title="{{ 'pel.footer.link.service-public.title'|trans }}" rel="noreferrer noopener" target="_blank" target="_blank" href="https://www.service-public.fr/">{{ 'pel.footer.link.service-public'|trans }}</a>
          </li>
          <li class="fr-footer__content-item">
            <a class="fr-footer__content-link" title="{{ 'pel.footer.link.data.title'|trans }}" rel="noreferrer noopener" target="_blank" href="https://www.data.gouv.fr/">{{ 'pel.footer.link.data'|trans }}</a>
          </li>
        </ul>
        </ul>
      </div>
    </div>
    <div class="fr-footer__bottom">
      <ul class="fr-footer__bottom-list">
        <li class="fr-footer__bottom-item">
          <a class="fr-footer__bottom-link" href="/infos/plan-du-site">{{ 'pel.footer.link-bottom.sitemap'|trans }}</a>
        </li>
        <li class="fr-footer__bottom-item">
          <a class="fr-footer__bottom-link" href="/infos/accessibilite">{{ 'pel.footer.link-bottom.accessibility'|trans }}</a>
        </li>
        <li class="fr-footer__bottom-item">
          <a class="fr-footer__bottom-link" href="/infos/mentions-legales">{{ 'pel.footer.link-bottom.legal'|trans }}</a>
        </li>
        <li class="fr-footer__bottom-item">
          <a class="fr-footer__bottom-link" href="/infos/donnees-personnelles">{{ 'pel.footer.link-bottom.privacy'|trans }}</a>
        </li>
        <li class="fr-footer__bottom-item">
          <a class="fr-footer__bottom-link" href="/infos/gestion-des-cookies">{{ 'pel.footer.link-bottom.cookies'|trans }}</a>
        </li>
      </ul>
      </ul>
      <div class="fr-footer__bottom-copy">
        <p>
          {{ 'pel.footer.copyright'|trans }}
          <a href="https://github.com/etalab/licence-ouverte/blob/master/LO.md" rel="noreferrer noopener" target="_blank" title="{{ 'pel.footer.copyright.license.title'|trans }}">{{ 'pel.footer.copyright.license'|trans }}</a></p>
      </div>
    </div>
  </div>
</footer>