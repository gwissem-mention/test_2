<!DOCTYPE html>
<html lang="fr">

<head>
  {% block header %}
  <meta http-equiv="Content-Type" content="text/html; charset=LATIN1">
  <!-- Meta, title, CSS, favicons, etc. -->
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <title>Bagware</title>

  <link href="{{asset("bundles/alstefihm/Ihm/pictures/flavicon_bagware.png")}}" rel="icon" type="image/png" />

  <!-- Bootstrap core CSS -->

  <link href="{{asset("bundles/alstefihm/libs/bootstrap/bootstrap-3.3.7-dist/css/bootstrap.min.css")}}" rel="stylesheet"/>
  <link href="{{asset("bundles/alstefihm/libs/bootstrap/bootstrap-datetimepicker-master/css/bootstrap-datetimepicker.min.css")}}" rel="stylesheet"/>

  <link href="{{asset("bundles/alstefihm/libs/font-awesome/font-awesome-4.7.0/css/font-awesome.min.css")}}" rel="stylesheet"/>
  <link href="{{asset("bundles/alstefihm/libs/animatecss/animate.css")}}" rel="stylesheet"/>

  <!-- Custom styling plus plugins -->
  <link href="{{asset("bundles/alstefihm/Ihm/css/main.css")}}" rel="stylesheet"/>
  <link href="{{asset("bundles/alstefihm/Ihm/css/tabs.css")}}" rel="stylesheet"/>
  <link href="{{asset("bundles/alstefihm/Ihm/css/indicators.css")}}" rel="stylesheet" type="text/css"/>
  <link href="{{asset("bundles/alstefihm/Ihm/css/datatable.css")}}" rel="stylesheet"/>

  <!-- DataTables -->
  <link href="{{asset("bundles/alstefihm/libs/DataTables/datatables.min.css")}}" rel="stylesheet" type="text/css"/>
  <link href="{{asset("bundles/alstefihm/libs/DataTables/FixedHeader-3.1.3/css/fixedHeader.bootstrap.min.css")}}" rel="stylesheet" type="text/css"/>
  <link href="{{asset("bundles/alstefihm/libs/DataTables/Responsive-2.2.1/css/responsive.bootstrap.min.css")}}" rel="stylesheet" type="text/css"/>
  <link href="{{asset("bundles/alstefihm/libs/DataTables/Scroller-1.4.3/css/scroller.bootstrap.min.css")}}" rel="stylesheet" type="text/css"/>

  <script src="{{asset("bundles/alstefihm/libs/jquery/jquery-3.2.1.min.js")}}" type="text/javascript"></script>
  <!-- Datatables-->
  <script src="{{asset("bundles/alstefihm/libs/DataTables/datatables.js")}}"></script>
  <script src="{{asset("bundles/alstefihm/libs/DataTables/FixedHeader-3.1.3/js/dataTables.fixedHeader.js")}}"></script>
  <script src="{{asset("bundles/alstefihm/libs/DataTables/Responsive-2.2.1/js/dataTables.responsive.js")}}"></script>
  <script src="{{asset("bundles/alstefihm/libs/DataTables/Responsive-2.2.1/js/responsive.bootstrap.js")}}"></script>
  <script src="{{asset("bundles/alstefihm/libs/DataTables/Scroller-1.4.3/js/dataTables.scroller.js")}}"></script>

  <script type="text/javascript" src="{{ asset('bundles/alstefihm/libs/jquery/jquery.jrumble.1.3/jquery.jrumble.1.3.min.js') }}"></script>
  <script type="text/javascript" src="{{ asset('bundles/alstefihm/libs/jquery/jquery.nicescroll-3.7.6/jquery.nicescroll.min.js') }}"></script>
  <!-- bootstrap -->
  <script src="{{asset("bundles/alstefihm/libs/Momentjs/Moment.js")}}" type="text/javascript"></script>
  <script src="{{asset("bundles/alstefihm/libs/bootstrap/bootstrap-3.3.7-dist/js/bootstrap.min.js")}}" type="text/javascript"></script>
  <script src="{{asset("bundles/alstefihm/libs/bootstrap/bootstrap-datetimepicker-master/js/bootstrap-datetimepicker.min.js")}}" type="text/javascript"></script>

  <!-- Script BAGWARE -->
  <script src="{{asset("bundles/alstefihm/Ihm/js/componentmanager/TabManager.js")}}"></script>
  <script src="{{asset("bundles/alstefihm/Ihm/js/componentmanager/DatatableManager.js")}}"></script>
  <script src="{{asset("bundles/alstefihm/Ihm/js/Utils.js")}}"></script>
  <script src="{{asset("bundles/alstefihm/Ihm/js/custom.js")}}"></script>
  <script src="{{asset("bundles/alstefihm/Ihm/js/screenmanager/ScreenManager.js")}}"></script>
  <script src="{{asset("bundles/alstefihm/Ihm/js/screenmanager/ScreenMainManager.js")}}"></script>
  <script src="{{asset("bundles/alstefihm/Ihm/js/Main.js")}}"></script>
  <script type="text/javascript">
    var traductions_datatables = {
        processing:     "{{ 'LIBELLE.DATATABLE_PROCESSING' | trans }}", 			// "Traitement en cours..."
        search:         "{{ 'LIBELLE.DATATABLE_SEARCH' | trans }}", 					// "Rechercher :"
        lengthMenu:     "{{ 'LIBELLE.DATATABLE_LENGTHMENU' | trans }}",				// "Afficher _MENU_ éléments"
        info:           "{{ 'LIBELLE.DATATABLE_INFO' | trans }}", 						// "Affichage de l'élément _START_ à _END_ sur _TOTAL_ éléments"
        infoEmpty:      "{{ 'LIBELLE.DATATABLE_INFOEMPTY' | trans }}",				// "Affichage de l'élément 0 à 0 sur 0 éléments"
        infoFiltered:   "{{ 'LIBELLE.DATATABLE_INFOFILTERED' | trans }}", 		// "(filtré de _MAX_ éléments au total)"
        infoPostFix:    "",
        loadingRecords: "{{ 'LIBELLE.DATATABLE_LOADINGRECORDS' | trans }}", 	// "Chargement en cours..."
        zeroRecords:    "{{ 'LIBELLE.DATATABLE_ZERORECORDS' | trans }}",			// "Aucun él&ément à afficher"
        emptyTable:     "{{ 'LIBELLE.DATATABLE_EMPTYTABLE' | trans }}",				// "Aucune donnée disponible dans le tableau"
        paginate: {
            first:      "{{ 'LIBELLE.DATATABLE_FIRST' | trans }}",						// "Premier"
            last:       "{{ 'LIBELLE.DATATABLE_LAST' | trans }}",							// "Dernier"
            previous:   "{{ 'LIBELLE.DATATABLE_PREVIOUS' | trans }}",					// "Précédent"
            next:       "{{ 'LIBELLE.DATATABLE_NEXT' | trans }}",							// "Suivant"
        },
        aria: {
            sortAscending:  "{{ 'LIBELLE.DATATABLE_SORTASCENDING' | trans }}",	// ": activer pour trier la colonne par ordre croissant"
            sortDescending: "{{ 'LIBELLE.DATATABLE_SORTDESCENDING' | trans }}"	// ": activer pour trier la colonne par ordre décroissant"
        }
    };
  </script>

  {% endblock %}
</head>

<body class="nav-md">
  <div class="container body">
    <div class="main_container">
      {% block leftnavbar %}
      {% endblock %}
      {% block topnavbar %}
      <!-- top navigation -->
      <div class="top_nav">

        <div class="nav_menu">
          <nav class="" role="navigation">
            <div class="nav toggle">
              <a id="menu_toggle">
                <i class="fa fa-bars"></i>
              </a>
            </div>

            <ul class="nav navbar-nav">
              <!-- Nav tabs -->
              <ul id="tabcontainer" class="nav nav-tabs" role="tablist">

              </ul>

            </ul>
          </nav>
        </div>

      </div>
      <!-- /top navigation -->
      {% endblock %}

      <!-- page content -->
      <div class="right_col" role="main" style="height: 100%">
        <!-- Tab panes -->
        <div class="tab-content" id="content" style="min-height: 860px">
          <img class="centerlogo" type="image/png" src="{{asset("bundles/alstefihm/Ihm/pictures/ALSTEF_fond_ecran_BAGWARE.jpg")}}"> {% block content %} {% endblock %}
        </div>

        {#
        <footer>
          {% block footer %} {% endblock %}
        </footer>#}
      </div>


      <!-- /page content -->
    </div>
  </div>
  <div class="indicators">
    <!-- class div = indic-ok, indic-warn ou indic-err, class i = fa-check, fa-exclamation ou fa-ban -->
    <div class="col-xs-1 col-sm-1 col-md-1" id="errorButton">    <i class="fa"></i>Err:0  </div>
    <div class="col-xs-1 col-sm-1 col-md-1" id="errexButton">    <i class="fa"></i>Erx:0  </div>
    <div class="col-xs-1 col-sm-1 col-md-1" id="errwarnButton">  <i class="fa"></i>Erw:0  </div>
    <div class="col-xs-1 col-sm-1 col-md-1" id="modesButton">    <i class="fa"></i>Mod:0  </div>
    <div class="col-xs-1 col-sm-1 col-md-1" id="stockButton">    <i class="fa"></i>Sto:0  </div>
    <div class="col-xs-1 col-sm-1 col-md-1" id="alarmButton">    <i class="fa"></i>Alr:0  </div>
    <div class="col-xs-1 col-sm-1 col-md-1" id="processButton">  <i class="fa"></i>Proc:0 </div>
    <!-- Impossible de mettre les 2 champs du milieu (date + serveur/PC) : bootstrap limite à 12 le nombre de cases horizontales
    <div class="col-xs-1 col-sm-1 col-md-1" id="dateField"><i class="fa"></i>yyyy-mm-dd hh:mi:ss</div>
    <div class="col-xs-1 col-sm-1 col-md-1" id="serveurField"><i class="fa"></i>es7-tba, Term: PC_ThierryB</div>
    -->
    <div class="col-xs-1 col-sm-1 col-md-1" id="materielButton"> <i class="fa"></i>Mat:1  </div>
    <div class="col-xs-1 col-sm-1 col-md-1" id="volsButton">     <i class="fa"></i>Vols:0 </div>
    <div class="col-xs-1 col-sm-1 col-md-1" id="mvtsButton">     <i class="fa"></i>Mvts:0 </div>
    <div class="col-xs-1 col-sm-1 col-md-1" id="linksButton">    <i class="fa"></i>Liens:1</div>
    <div class="col-xs-1 col-sm-1 col-md-1" id="eqptButton">     <i class="fa"></i>Eqt:1  </div>
  </div>
  <script type="text/javascript">
    initAlstefIHM();
  </script>
</body>

</html>
